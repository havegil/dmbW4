.SidesMenu {
  background: #fff;
  padding: 27px 0 0 30px;
  width: 100%; height: 100%;

  & .c-Header_column:nth-child(1),
  & .c-TableList_column:nth-child(1) {
    width: 72%;
  }

  & .c-Header_column:nth-child(2),
  & .c-TableList_column:nth-child(2) {
    width: 28%;
  }

  .c-TableList_row {
    padding: 0px 0px 20px 0px;
  }

  .served-until-11 {
    position: absolute;
    right: 63px;
    bottom: 20px;
    color: #008938;
    font-size: 36px;
    font-family: 'Subway-Medium';

    .asterisk {
      position: relative;
      top: 0px;
      left: 5px;
    }
  }

  /* Legal updates */
  .c-Menus_header {
    margin-bottom: 10px;
  }

  .c-Menus_caption {
    font-size: 36px;
  }

  /* legal updates for font size */
  .c-Menus_title, .c-Menus_caption-highlight {
    font-size: 36px;
  }

  sup {
    position: relative;
    font-size: 18px !important;
  }
}
