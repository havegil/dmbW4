.ClubInterrupt {
  background: rgba(0,0,0,0.8);
  position: absolute;
  bottom: 0px;
  left: 0px;
  width: 7680px;
  height: 1080px;

  .club-text {
    opacity: 0;
  }

  .club-frame {
    top: 1080px;
  }

  img {
    position: absolute;
    z-index: 99;
    left: 0px;
    opacity: 1;


    &.f1 {
      left:0;
    }

    &.f2 {
      left: 1920px;
    }

    &.f3 {
      left: 3840px;
    }

    &.f4 {
      left: 5760px;
    }
  }

}
