
.screen-3 {
  width: 100%;
  height: 100%;

  .s-Body {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: calc(100% - 200px);
  }

  .s-Footer {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 200px;
    background: #008938;
  }
}

