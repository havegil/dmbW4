.c-SignatureWrapsMenu {
  padding: 40px 40px 0px 40px;
  height: 100%;

  & .c-TableList_column:nth-child(1),
  & .c-TableList_column:nth-child(2),
  & .c-TableList_column:nth-child(3) {
    padding-right: 20px;
  }

  & .c-Header_column:nth-child(1),
  & .c-TableList_column:nth-child(1) {
    width: 86%;
    /*padding-left: 1%;*/
  }

  & .c-Header_column:nth-child(2),
  & .c-TableList_column:nth-child(2) {
    width: 13%;
  }

  & .c-Header_column:nth-child(3),
  & .c-TableList_column:nth-child(3) {
    width: 17%;

    .c-Menus_title {
      margin-left: 40px;
    }
  }

  .wrapImage {
    position: absolute;
    top: 205px;
    transition: all 500ms;
  }

  .c-Menus_caption {
    /*font-size: 1.7rem;*/
    font-size: 36px;
    display: inline;
  }

  /* legal updates for font size */
  .c-Menus_title, .c-Menus_caption-highlight {
    font-size: 36px;
    line-height: 1.1;
  }

  .c-Sub_caption {
    width: 100%;
    text-align: left;
  }

  .c-TableList_row {
    /*padding: 5px 5px 0px 5px;*/
    /*margin-bottom: 2px;*/
    top: 230px;
  }

  .c-TableList_row .sub_title {
    display: inline !important;
  }

  .headerCal {
    padding-top: 23px;
    margin-right: 3px;
  }

  .cals {
    display: inline;
  }

  .c-Menus_header {
    padding-right: 20px;
    /* This is lame, but the header is not lining up with the line items */
    /*margin-left: -3px;*/
    color: #fff;
  }

  .legalContent {
    font-family: 'SubwayTest-Medium';
    position: relative;
    left: -13px;
    font-size: 18px;
  }

  .c-Menus_subtitle {
    width: 91%;
  }

  .wrap-promo-panel {
    position: absolute;
    top: 0;
    right: 0;
  }


  .protein-bowls-side-panel {
    position: absolute;
    top: 0;
    right: 0;
    height: 1080px;
    width: 640px;

    img {
      position: absolute;
      top: 0;
      right: 0;
      animation-name: bowls-fade;
      animation-duration: 60s;


      animation-iteration-count: infinite;
      opacity: 0;
    }

  }

}

@keyframes bowls-fade {
  0% {opacity: 1;}
  49% {opacity: 1;}
  50% {opacity: 0;}
  99% {opacity: 0;}
  100% {opacity: 1;}
}

.r-showChicken {
  .c-rotisserie_chicken_caesar_wrap span {color: #f7b301}

  .imgChicken {opacity: 1}
  .imgSteak   {opacity: 0}
  .imgTurkey  {opacity: 0}
}

.r-showSteak {
  .c-chipotle_southwest_steak_and_cheese_wrap span {color: #f7b301}

  .imgChicken {opacity: 0}
  .imgSteak   {opacity: 1}
  .imgTurkey  {opacity: 0}
}

.r-showTurkey {
  .c-turkey_bacon_guacamole_wrap span {color: #fff}

  .imgChicken {opacity: 0}
  .imgSteak   {opacity: 0}
  .imgTurkey  {opacity: 1}
}

.rb-sku-test {
  .c-turkey_bacon_guacamole_wrap span {color: #fff !important}
  .c-chipotle_southwest_steak_and_cheese_wrap span {color: #fff !important}
  .c-rotisserie_chicken_caesar_wrap span {color: #fff !important}
}


.avocado-wrap {
  .c-TableList_row {
    /*padding: 5px 5px 0px 5px;*/
    /*margin-bottom: 2px;*/
    top: 30px;
  }
}