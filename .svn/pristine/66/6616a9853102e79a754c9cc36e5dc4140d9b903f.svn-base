.c-ucgb3-dual {
	img {
		position: absolute;
		top: 0;
		left: 0;
	}

	.text-box {
		font-family: 'Subway-Heavy', 'sans-serif';
		font-size: 100px;
		width: 1280px;
		position: absolute;
		top: 0;
		left: 0;
		text-transform: uppercase;
		text-align: center;
		line-height: 1;

		.headline {
			display: block;
			text-align: center;
			position: absolute;
			width: 1280px;
			line-height: 1;
			text-shadow: 2px 2px 20px rgba(0, 0, 0, 0.5);
		}
	}

	.price {
		font-family: 'Subway-Heavy', 'sans-serif';
		font-size: 37px;
		width: 1280px;
		display: block;
	}

	.green {
		color: #00572D;
		line-height: 1;
	}

	.green-bg {
		color: #F2B700;
		background-color: #00572D;
		margin-left: 30px;
		line-height: 1;
	}

	.price-container {
		position: absolute;
		top: 0;
		left: 0;
		font-family: 'Subway-Heavy', 'sans-serif';
		font-size: 37px;
		width: 500px;
	}

	.italian-price {
		top: 335px;
		left: 70px;
	}

	.meatball-price {
		top: 560px;
		left: 820px;
	}

	.steak-price {
		top: 980px;
		left: 930px;
	}

	.cal {
		font-family: 'Subway-Medium', 'sans-serif';
		margin-left: 20px;
	}
	
}