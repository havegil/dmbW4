$framedelay: 15000ms;
$framedelay1: 15100ms;
$framedelay2: 15200ms;
$framedelay3: 15300ms;
$framedelay4: 15400ms;
$framedelay5: 15500ms;

@keyframes fadeInKidsMeal {
  from {
    opacity: 0;
  }
  4% {
    opacity: 1;
  }
  to {
    opacity: 1;
  }
}

@keyframes fadeInOutKidsMeal {
  from {
    opacity: 0;
  }
  4% {
    opacity: 1;
  }
  96% {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}

@keyframes moveToOrigin {
  20% {
    transform: translate3d(0, 0, 0) rotate3d(0,0,0,0deg);
  }
  to {
    transform: translate3d(0, 0, 0) rotate3d(0,0,0,0deg);
  }
}

@keyframes moveToInitial {
  to {
    transform: initial;
  }
}

@keyframes skyLogo {
  9% {
    transform: translate3d(0, 0, 0) rotate3d(0,0,0,0deg);
  }
  82% {
    transform: translate3d(0, 0, 0) rotate3d(0,0,0,0deg);
  }
  95% {
    transform: translate3d(678px, 103px, 0) scale(0.833);
  }
  to {
    transform: translate3d(678px, 103px, 0) scale(0.833);
  }
}

@keyframes mixieLogo {
  14% {
    transform: translate3d(0, 0, 0) rotate3d(0,0,0,0deg);
  }
  82% {
    transform: translate3d(0, 0, 0) rotate3d(0,0,0,0deg);
  }
  95% {
    transform: translate3d(663px, -22px, 0) scale(0.833);
  }
  to {
    transform: translate3d(663px, -22px, 0) scale(0.833);
  }
}

@keyframes meal {
  10% {
    transform: translate3d(0, 0, 0) rotate3d(0,0,0,0deg);
  }
  82% {
    transform: translate3d(0, 0, 0) rotate3d(0,0,0,0deg);
  }
  95% {
    transform: translate3d(-35px, -31px, 0) scale(0.895);
  }
  to {
    transform: translate3d(-35px, -31px, 0) scale(0.895);
  }
}


.kids-dual  {

  * {
    animation: none !important;
    transform: translate3d(0, 0, 0) rotate3d(0,0,0,0deg) !important;
  }

}

.c-KidsMeal {
