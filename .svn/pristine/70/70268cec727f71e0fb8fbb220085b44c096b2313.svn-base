      animation-iteration-count: 1;
    }

    .t-watch {
      top: 296px;
      left: 174px;
      transform: translate3d(0, 200px, 0);
      animation: moveToOrigin 4700ms ease-out $framedelay3;
      animation-iteration-count: 2;
