const isNotPromoCiabatta = (promoNum) => ({
  name: `Show if promo is not Ciabatta`,
  predicate: () => (
    numToPromo[promoNum] != 'ciabatta_collection' &&
    numToPromo[promoNum] != 'ciabatta_collection_caprese'
  )
})

