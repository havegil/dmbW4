.c-breakfastFooter {
  position: relative;
  width: 100%; height: 100%;
  left: 0; top: 0; right: 0; bottom: 0;

  .c-TableList_column:nth-child(1) {
    width: 80%;
  }

  .sub_title  {
    margin-right: 10px;
  }

  .c-Menus_header {
    font-size: 41px;
    color: #fff;
    padding-bottom: 15px;
  }

  .cals span {
    display: none;
  }

  .servedUntil11 {
    position: absolute;
    top: 160px;
    left: 30px;
  }

  .sides-wrapper {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
  }

  .sandwich {
    position: absolute;
    top: 140px;
    left: 150px;
  }

  .coffee {
    position: absolute;
    top: 0px;
    right: 35px;
  }

  .meal {
    position: absolute;
    top: -40px;
    right: 5px;
  }

  .clarify {
    position: absolute;
    left: 0;
    bottom: -20px;
    font-family: 'Subway-Medium';
    font-size: 22px;
    color: #fff;
  }

}

.videoTest {
    position: absolute;
    top: 0;
    left: 0%;
    width: 50%;
}

.gifTest {
    position: absolute;
    top: 0% !important;
    left: 0%;
    width: 60%;
    height: 50%;
    opacity: 1 !important;
    z-index: 100;
}

.rb-sku-test {
  .clarify {
    font-size: 32px;
    line-height: 1.1;

    span {
      display: block;
      font-size: 18px;
    }
  }
}
