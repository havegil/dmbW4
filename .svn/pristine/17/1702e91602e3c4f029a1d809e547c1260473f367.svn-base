.c-FeaturedPromotionTurkeyReuben2016w8 {
	position    : relative;
	width       : 100%;
  height      : 100%;
  overflow    : hidden;
  font-family : 'MarkOffcPro-Bold';
  font-size   : 2.8rem;
	line-height : 2.1rem;
	color       : #6d6e71;
}

.c-FeaturedPromotionTurkeyReuben2016w8_options {
	position : absolute;
	width    : 40%;
	bottom   : 10px;
	right    : 1%;
  z-index  : 2;
}


.c-AdjacentList {
  position  : relative;
  width     : 100%;
  height    : auto;
  overflow  : visible;
  font-size : 0px;
}

.c-AdjacentList_size {
  position    : relative;
  display     : inline-block;
  width       : 50%;
  font-size   : 2.8rem;
  line-height : 2.1rem;
  color       : #6d6e71;

  &-sm {
    .c-AdjacentList_size_heading,.c-AdjacentList_size_values {
      font-size   : 1.8rem;
      line-height : 1.8rem;
    }
  }
  &-md {

  }
  &-lg {
    .c-AdjacentList_size_heading,.c-AdjacentList_size_values {
      font-size   : 2.8rem;
      line-height : 2.8rem;
    }
  }
}

.c-AdjacentList_size_heading {
  position       : relative;
  display        : inline-block;
  padding-right  : 20%;
  vertical-align : top;
  text-align     : center;
  font-family    : 'MyriadPro-BoldCondensed';
}

.c-AdjacentList_size_values {
  position    : relative;
  display     : inline-block;
  font-family : 'MyriadPro-Condensed';
}