.c-Promotion-2018w3-CheesesteakPromo {

    height: 1080px;
    width: 100%;
    display: inline-block;
    background: #fff;
    position: relative;
    font-size: 36px;
    color: #fa4c16;
    font-family: 'Subway-Heavy';


    .steakFrame {
      position: absolute;
      left: 0;
      top: 0;
      height: 100%;
      width: 100%;
      opacity: 0;
    }

    .startFrame {
      z-index: 1000;
    }

    .frame1 {
      z-index: 2;
    }

    .frame2 {
      z-index: 3;
    }

    .frame3 {
      z-index: 4;
    }

    .frame4 {
      z-index: 5;
    }

    .frame5 {
      z-index: 6;
    }

    .frame6 {
      z-index: 7;
    }

    .finalFrame {
      position: absolute;
      left: 0;
      top: 0;
      width: 100%;
      height: 100%;
      z-index: 8;
    }

    .price {
      font-family: 'Subway-Heavy';
      font-size: 45px;
      color: #ffffff;
      width: 500px;
      position: absolute;
    }

    .price1 {
      left: 72px; top: 570px;
    }

    .price2 {
      left: 724px; top: 421px;
    }

    .price3 {
      left: 72px; top: 810px;
    }

  }
