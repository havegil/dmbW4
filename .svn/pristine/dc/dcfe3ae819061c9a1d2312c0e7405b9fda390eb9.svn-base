.c-TableList_row {
  position: relative;
  
  .decal {
    position: absolute;
    height: 66px;
    width: 66px;
    top: -20px;
    border-radius: 33px;
    text-align: center;
    line-height: normal;

    .price {
      margin: 15px 0 0 0 !important;

      .sign,
      .dollars,
      .cents {
        color: #fff;
        display: inline-block;
        vertical-align: top;
      }

      .sign {
        font-size: 18.5px;
        margin: 1px -4px 0 0;
      }

      .dollars {
        font-size: 33px;
      }

      .cents {
        font-size: 18.5px;
        margin: 1px 0 0 -4px;
      }
    }

    .label {
      margin: -8px 0 0 0 !important;
      color: #fff;
      font-size: 10.5px;
    }
  }

  .decal6 {
    left: -77px;
  }

  .decal12 {
    right: 15px;
  }

  .c-not-available span {
    font-size: 20px;
    display: inline-block;
    text-align: center;
    margin-top: -2px;
  }

  .blue {
    background: #00a5de;
  }

  .green {
    background: #85c441;
  }
}

.c-Menus_title span {
  padding-right: 3px;
}
